  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bootstrap": "^4.1.2",
    "bootstrap-vue": "^2.0.0-rc.11",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.24.0",
    "mongoose": "^5.5.1",
    "popper.js": "^1.14.3",
    "url-parse": "^1.4.3",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.4",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.4",
    "@vue/cli-service": "^3.0.0-rc.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
